{
  "name": "goo-token-website",
  "version": "1.0.0",
  "description": "Professional website for Goo Token cryptocurrency - Developed by ErsanJ.Tabrizi from Bizdavar Team",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=3000 --open=/index.html",
    "build": "npm run typecheck && npm run minify-css && npm run minify-js",
    "minify-css": "cleancss -o dist/styles.min.css styles.css",
    "minify-js": "terser script.js -o dist/script.min.js",
    "deploy": "npm run build && gh-pages -d .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "backup": "node scripts/backup.js",
    "clean": "rimraf dist node_modules/.cache",
    "prebuild": "npm run clean",
    "precommit": "npm run typecheck && npm run lint && npm run test"
  },
  "keywords": [
    "cryptocurrency",
    "token",
    "blockchain",
    "website",
    "goo-token"
  ],
  "author": "ErsanJ.Tabrizi - Bizdavar Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-jest": "^30.2.0",
    "clean-css-cli": "^5.6.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "gh-pages": "^6.3.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^30.2.0",
    "live-server": "^1.2.2",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "terser": "^5.19.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/goo-token-website.git"
  },
  "homepage": "https://yourusername.github.io/goo-token-website"
}
